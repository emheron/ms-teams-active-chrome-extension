{
  "manifest_version": 3,
  "name": "Wiggle Teams",
  "version": "1.0",
  "description": "Wiggle Teams",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["*://teams.microsoft.com/*"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Wiggle Teams!",
    "default_icon": {
      "128": "activeUser.png"
    }
  }
}



